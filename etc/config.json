{
  "logging": {
    "categories": "config,listen,path,parse",
    "prefix": "--",
    "timestamp": true
  },
  "listen": {
    "host": "localhost",
    "port": 12368
  },
  "channels": {
    "badtype": {
      "type": "scraper"
    },
    "nohost": {
      "type": "rss",
      "url": "https://nosuchfeeds.bbci.co.uk/news/rss.xml"
    },
    "nofile": {
      "type": "rss",
      "url": "https://feeds.bbci.co.uk/news/nosuchrss.xml"
    },
    "static": {
      "type": "rss",
      "url": "file:///static/rss.xml"
    },
    "bbc": {
      "type": "rss",
      "url": "https://feeds.bbci.co.uk/news/rss.xml",
      "render": "description"
    },
    "hackernews": {
      "type": "rss",
      "url": "https://news.ycombinator.com/rss",
      "render": "title"
    },
    "svpow": {
      "type": "rss",
      "url": "https://svpow.com/feed/",
      "render": "title"
    }
  },
  "transformations": {
    "dumbass": {
